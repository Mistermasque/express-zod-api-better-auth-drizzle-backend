{
  "name": "backend",
  "version": "1.0.0",
  "packageManager": "yarn@4.6.0",
  "main": "src/main.ts",
  "dependencies": {
    "@prisma/client": "6.12.0",
    "better-auth": "^1.3.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-session": "^1.18.2",
    "express-zod-api": "^24.7.1",
    "http-errors": "^2.0.0",
    "node-forge": "^1.3.1",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/cookie-parser": "^1",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-session": "^1.18.2",
    "@types/http-errors": "^2.0.5",
    "@types/node": "^24.1.0",
    "@types/node-forge": "^1",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "6.12.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "tspath": "^2.6.8",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "typescript-transform-paths": "^3.5.5",
    "zipfs": "^1.0.3"
  },
  "scripts": {
    "dev": "ts-node-dev --require tsconfig-paths/register src/main.ts",
    "dev:debug": "ts-node-dev --require tsconfig-paths/register --inspect=9228 src/main.ts",
    "build": "tspc",
    "build:doc": "tsx src/utils/generate-doc.ts",
    "build:client": "tsx src/utils/generate-client.ts",
    "start": "ts-node --require tsconfig-paths/register src/main.ts",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "db:auth-schema": "./scripts/create-auth-cli-files.bash && npx @better-auth/cli generate --config ./tmp/auth-cli/auth/auth.ts --output ./src/db/schemas/auth-schema.ts",
    "db:generate": "drizzle-kit generate --config=./src/config/drizzle-config.ts",
    "db:migrate": "drizzle-kit migrate --config ./src/config/drizzle-config.ts",
    "db:push": "drizzle-kit push --config=./src/config/drizzle-config.ts",
    "db:studio": "drizzle-kit studio"
  }
}
